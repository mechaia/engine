% T A B C

$ t T
$ b Int32
$ c ConstStr

> f
| f_switch
= write.newline

[ f_switch t
? A f_a
? B f_b
? C f_c
]

> f_a 
+ int8:0 'a'
= write.byte

> f_b 
+ int8:0 'b'
| write.byte
| f_arrow
. int32:0 b
= write.int32

> f_c
+ int8:0 'c'
| write.byte
| f_arrow
. const_str:0 c
= write.const_str

> f_arrow
+ const_str:0 " -> "
= write.const_str

> start
+ t A
| f
+ t B
+ b 42
| f
+ t C
+ c "Yoyoyo"
= f
