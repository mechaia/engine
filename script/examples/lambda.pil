import { range, write_stdout, Env }
export { main }

function main(Env env) -> () {
    (Env) {
        lambda f(Integer i) -> () {
            (Integer) {
                var IntegerToStringBuffer buf
                integer_to_string(i) -> (buf)
                write_stdout(env, buf) -> (env)
            }
        }
        range_until(0, 3, f)
    }
}
